{
  "name": "@qiaopeng/tanstack-query-plus",
  "version": "0.2.11",
  "description": "Enhanced TanStack Query toolkit: defaults, hooks, persistence, offline, utils",
  "author": "qiaopeng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiaopengg/qiaopeng-tanstack-query-plus.git"
  },
  "bugs": {
    "url": "https://github.com/qiaopengg/qiaopeng-tanstack-query-plus/issues"
  },
  "homepage": "https://github.com/qiaopengg/qiaopeng-tanstack-query-plus#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "tanstack",
    "react-query",
    "query",
    "hooks",
    "persistence",
    "offline",
    "suspense",
    "devtools"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react-query": {
      "import": "./dist/react-query/index.js",
      "types": "./dist/react-query/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./core/devtools": {
      "import": "./dist/core/devtools.js",
      "types": "./dist/core/devtools.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./hooks/inview": {
      "import": "./dist/hooks/useInViewPrefetch.js",
      "types": "./dist/hooks/useInViewPrefetch.d.ts"
    },
    "./features": {
      "import": "./dist/features/index.js",
      "types": "./dist/features/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "@tanstack/react-query-devtools": "^5",
    "@tanstack/react-query-persist-client": "^5",
    "react": ">=18",
    "react-dom": ">=18",
    "react-intersection-observer": "^9 || ^10"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query-devtools": {
      "optional": true
    },
    "react-intersection-observer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "react-intersection-observer": "^9",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  }
}
